<h3>Create a User</h3>
<%= form_for @user do |f| %>
	<%= f.label :name %><br>
	<%= f.text_field :name%><br>
	<%= f.label :email %><br>
	<%= f.text_field :email %><br>
	<%= f.label :ssn %><br>
	<%= f.text_field :ssn %><br>
	<%= f.label :standing %><br>
	<%= f.text_field :standing %><br>
	<%= f.submit %>
<%end%>